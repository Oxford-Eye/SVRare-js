{
  "name": "svrare-js",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material/react-radio": "^0.15.0",
    "axios": "^0.26.1",
    "cors": "^2.8.5",
    "d3": "5.16.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "igv": "^2.12.2",
    "jotai": "^1.6.6",
    "jquery": "^3.6.0",
    "pedigreejs": "git+https://github.com/CCGE-BOADICEA/pedigreejs",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-table": "^7.7.0",
    "sequelize": "^6.17.0",
    "sequelize-auto": "^0.8.8",
    "serve": "^13.0.2",
    "sqlite3": "mapbox/node-sqlite3#593c9d498be2510d286349134537e3bf89401c4a",
    "styled-components": "^5.3.5"
  },
  "scripts": {
    "server-dev": "ts-node-dev --respawn src/server.ts",
    "copy-webconfig": "cp asset/webpack.config.*.js node_modules/react-scripts-ts/config/",
    "start": "export ENV_FILE=.env.local && npm run copy-webconfig && concurrently --kill-others \"react-scripts-ts start\" \"ts-node-dev --respawn src/server.ts\"",
    "build": "export ENV_FILE=.env rm -rf build/ && npm run copy-webconfig && react-scripts-ts build && tsc --project tsconfig.server.json && concurrently --kill-others \"serve -s build\" \"node build/server.js\"",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-table": "^7.7.10",
    "@types/styled-components": "^5.1.25",
    "concurrently": "^7.1.0",
    "react-scripts-ts": "^3.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
